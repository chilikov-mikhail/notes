<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="NoteForm">
  <grid id="27dc6" binding="notePanel" layout-manager="BorderLayout" hgap="5" vgap="5">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <hspacer id="964a0">
        <constraints border-constraint="East"/>
      </hspacer>
      <hspacer id="d7dd9">
        <constraints border-constraint="West"/>
      </hspacer>
      <grid id="22b23" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="20a36" class="javax.swing.JButton" binding="saveButton" default-binding="true">
            <constraints/>
            <properties>
              <text value="Сохранить заметку"/>
            </properties>
          </component>
          <component id="a5e1f" class="javax.swing.JButton" binding="deleteButton">
            <constraints/>
            <properties>
              <text value="Удалить заметку"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="4aa89" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="c30e6" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="0" left="0" bottom="0" right="0"/>
            <constraints border-constraint="North"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="ec52c" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="Заголовок заметки (только буквы, цифры, пробелы, скобки, №, _, -):"/>
                </properties>
              </component>
              <component id="f870c" class="javax.swing.JTextField" binding="title">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                    <preferred-size width="150" height="-1"/>
                  </grid>
                </constraints>
                <properties/>
              </component>
            </children>
          </grid>
          <grid id="c84ee" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="0" left="0" bottom="0" right="0"/>
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="3aff9" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="Текст заметки (любые символы):"/>
                </properties>
              </component>
              <component id="f3019" class="javax.swing.JTextArea" binding="text">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="6" anchor="0" fill="3" indent="0" use-parent-layout="false">
                    <preferred-size width="150" height="50"/>
                  </grid>
                </constraints>
                <properties/>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <vspacer id="3b92f">
        <constraints border-constraint="North"/>
      </vspacer>
    </children>
  </grid>
</form>
